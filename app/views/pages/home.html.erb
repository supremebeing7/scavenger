<%= content_for :title, "Home" %>

<div class="row">
  <div class="col-sm-2">
    <%= link_to "Add Hunt", new_crawl_path, class: "btn btn-custom" %>
  </div>
  <div class="col-sm-8">
    <h2>CRAWLS</h2>
    <% @crawls.each do |crawl| %>
      <p><%= link_to crawl.name, crawl %></p>
      <p><%= crawl.description %></p>
    <% end %>
  </div>
</div>
